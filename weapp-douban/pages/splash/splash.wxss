/* splash.wxss */
.container {
  height: 100%;
  overflow: hidden;
}

.splash {
  height: 100%;
}

.splash swiper-item {
  flex: 1;
}

swiper-item image {
  position: absolute;
  height: 100%;
  width: 100%;
  opacity: .9;
}

.start {
  position: absolute;
  bottom: 100rpx;
  left: 50%;
  width: 300rpx;
  margin-left: -150rpx;
  background-color: #43BB57;
  color: #fff;
  font-size: 40rpx;
  height: 82rpx;
  line-height: 82rpx;
}